    <link rel="stylesheet" type="text/css" href="libs/noty/noty.css"> -->
    <link rel="stylesheet" type="text/css" href="{{ asset('css/panels.min.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('css/payment.min.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('css/tabbed-sidebar.min.css') }}">
    {% block styles %}{% endblock %}
    <!-- END THEME STYLES -->

</head>
<body class="ks-navbar-fixed ks-sidebar-empty ks-sidebar-position-fixed ks-page-header-fixed ks-theme-primary ks-page-loading">
{% include 'templates/partials/dash_nav.twig' %}
{% include 'templates/partials/dash_menu.twig' %}
{% include 'templates/partials/alerts.twig'%}
{% block content %}
